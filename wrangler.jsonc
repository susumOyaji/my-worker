{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "my-worker",
  "main": "worker/build/index.js",
  "compatibility_date": "2025-09-13",
  "build": {
    "command": "cargo install -q worker-build && worker-build --release",
	"cwd": "./worker"
  },
  "observability": {
    "enabled": true
  },
  "vars": {
    "MY_VARIABLE": "secret_value"
  }
}
